{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "BackupOptions": {
    "BackupDirectory": "./backups",
    "CompressBackups": false,
    "IncludeSchema": true,
    "MaxBackupRetentionCount": 5,
    "BackupFileNameFormat": "wa_dev_backup_{0:yyyyMMdd_HHmmss}"
  }
}
